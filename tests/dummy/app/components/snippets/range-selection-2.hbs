<PowerCalendarRange
  class="demo-range-calendar-with-pretty-range-ends-yo"
  @center={{this.center}}
  @selected={{this.range}}
  @onCenterChange={{action (mut this.center) value="date"}}
  @onSelect={{action (mut this.range) value="date"}} as |calendar|>
  <calendar.Nav/>
  <calendar.Days/>
</PowerCalendarRange>
