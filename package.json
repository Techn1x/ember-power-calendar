{
  "name": "ember-power-calendar",
  "version": "0.20.2",
  "description": "Powerful and customizable calendar component for Ember",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:scss": "stylelint \"**/*.scss\" --allow-empty-input",
    "lint:scss:fix": "concurrently \"npm:lint:scss -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0 || >=5.0.0",
    "ember-power-calendar-moment": "^0.3.0",
    "ember-power-calendar-luxon": "^0.5.0"
  },
  "peerDependenciesMeta": {
    "ember-power-calendar-moment": {
      "optional": true
    },
    "ember-power-calendar-luxon": {
      "optional": true
    }
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "files": [
    "addon/",
    "app/",
    "addon-test-support/",
    "test-support/",
    "index.js"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "ember-basic-dropdown": "^7.2.1",
    "ember-cli": "~4.12.0",
    "ember-cli-app-version": "^6.0.1",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^11.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-code-example-component": "0.4.1",
    "ember-code-snippet": "git+https://git@github.com/ef4/ember-code-snippet.git#d054b697098ad52481c94a952ccf8d89ba1f25fe",
    "ember-data": "~4.12.0",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-power-calendar-moment": "git+https://git@github.com/mkszepp/ember-power-calendar-moment.git#3acc8d29f02c8a5d3970987727f09c37c69ecabe",
    "ember-power-select": "^7.1.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^11.0.0",
    "ember-source": "~4.12.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.1",
    "ember-try": "^2.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.10.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-qunit": "^8.0.0",
    "loader.js": "^4.7.0",
    "memory-scroll": "^1.0.2",
    "moment": "^2.29.4",
    "prettier": "^3.0.1",
    "prismjs": "^1.29.0",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-prettier": "^4.0.2",
    "tracked-built-ins": "^3.1.1",
    "webpack": "^5.88.2"
  },
  "keywords": [
    "ember-addon",
    "ember-power",
    "calendar",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/cibernox/ember-power-calendar.git"
  },
  "license": "MIT",
  "author": "Miguel Camba",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/macros": "^1.13.1",
    "ember-assign-helper": "^0.4.0",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-concurrency": "^3.1.0",
    "ember-element-helper": "^0.8.2",
    "ember-truth-helpers": "^3.1.0"
  },
  "overrides": {
    "ember-source": "$ember-source"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
